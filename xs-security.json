{
  "xsappname": "galactic-spacefarer-adventure",
  "tenant-mode": "dedicated",
  "description": "Security profile of called application",
  "scopes": [
    {
      "name": "uaa.user",
      "description": "UAA"
    },
    {
      "name": "$XSAPPNAME.Admin",
      "description": "Admin"
    }        
  ],
  "attributes": [
    {
      "name": "Planet",
      "description": "Planet",
      "valueType": "string",
      "valueRequired" : "false" 
    }
  ],  
  "role-templates": [
    {
      "name": "Token_Exchange",
      "description": "UAA",
      "scope-references": [
        "uaa.user"
      ]
    },
    {
      "name": "Admin",
      "description": "generated",
      "scope-references": ["$XSAPPNAME.Admin"],
      "attribute-references": []
    },
    {
      "name": "PlanetUser",
      "description": "Planet User Role Template",
      "default-role-name": "Attributes of a User",
      "scope-references": [],
      "attribute-references": [
        { "name": "Planet" }
      ]
    }    
  ],
  "role-collections": [
    {
      "name": "RCGalacticAdmin",
      "description": "Admin Can Create, Update, Delete The Data",
      "role-template-references": ["$XSAPPNAME.Admin"]
    }
  ],
  "oauth2-configuration": {
    "token-validity": 3600,
    "redirect-uris": ["https://*/**"]
  }  
}
